  val myString = ""  
    var listTurns = myString.toCharArray().toList().map {Turn.valueOf(it.toString())}
    
   
    
    fun nextButton (previousButton: Button, turn: Turn): Button =
    when (turn){
    U -> when (previousButton){
   		b1 -> b1
    	b2 -> b2
    	b3 -> b3
        b4 -> b1
        b5 -> b2
        b6 -> b3
        b7 -> b4
        b8 -> b5
        b9 -> b6
                
    }
    D -> when (previousButton){
   		b1 -> b4
    	b2 -> b5
    	b3 -> b6
        b4 -> b7
        b5 -> b8
        b6 -> b9
        b7 -> b7
        b8 -> b8
        b9 -> b9               
    }
            L -> when (previousButton){
   		b1 -> b1
    	b2 -> b1
    	b3 -> b2
        b4 -> b4
        b5 -> b4
        b6 -> b5
        b7 -> b7
        b8 -> b7
        b9 -> b8               
    }
         R -> when (previousButton){
   		b1 -> b2
    	b2 -> b3
    	b3 -> b3
        b4 -> b5
        b5 -> b6
        b6 -> b6
        b7 -> b8
        b8 -> b9
        b9 -> b9               
    }
    }
    
    fun endButton (firstButton: Button, listTurns: List<Turn>): Button{
        var endButton = firstButton
         for (i in listTurns){
            endButton = nextButton(endButton, i)
        }
        return endButton
    }
    
     print (endButton(b5, listTurns))
        
    
}

enum class Turn {
    U, D, R, L;
}

enum class Button{
    b1,b2, b3, b4, b5, b6, b7, b8, b9;
}
